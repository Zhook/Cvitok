<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DecoratedSeekBar">
        <attr name="description" format="string"/>
        <attr name="minValue" format="integer"/>
        <attr name="maxValue" format="integer"/>
        <attr name="minLabel" format="string"/>
        <attr name="maxLabel" format="string"/>
        <attr name="defValue" format="integer"/>
        <attr name="postfix" format="string"/>
    </declare-styleable>
    <declare-styleable name="Preference">
        <attr name="summaryFormat" format="string"/>
    </declare-styleable>
    <declare-styleable name="ColorPickerLayout">
         <attr name="defRed" format="float"/>
        <attr name="defGreen" format="float"/>
        <attr name="defBlue" format="float"/>
        <attr name="defAlpha" format="float"/>
    </declare-styleable>
    <declare-styleable name="TwoDecoratedSeekBars">
        <attr name="key1" format="string"/>
        <attr name="description1" format="string"/>
        <attr name="minValue1" format="integer"/>
        <attr name="maxValue1" format="integer"/>
        <attr name="minLabel1" format="string"/>
        <attr name="maxLabel1" format="string"/>
        <attr name="defValue1" format="integer"/>
        <attr name="postfix1" format="string"/>
        <attr name="key2" format="string"/>
        <attr name="description2" format="string"/>
        <attr name="minValue2" format="integer"/>
        <attr name="maxValue2" format="integer"/>
        <attr name="minLabel2" format="string"/>
        <attr name="maxLabel2" format="string"/>
        <attr name="defValue2" format="integer"/>
        <attr name="postfix2" format="string"/>
    </declare-styleable>
    <declare-styleable name="ColorDialog">
        <attr name="keyRed" format="string"/>
        <attr name="defValueRed" format="integer"/>
        <attr name="keyGreen" format="string"/>
        <attr name="defValueGreen" format="integer"/>
        <attr name="keyBlue" format="string"/>
        <attr name="defValueBlue" format="integer"/>
        <attr name="keyAlpha" format="string"/>
        <attr name="defValueAlpha" format="integer"/>
    </declare-styleable>
</resources>